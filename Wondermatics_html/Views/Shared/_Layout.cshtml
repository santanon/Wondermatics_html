<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Wondermatics_html</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/main.min.css" asp-append-version="true" />
</head>
<body>
    
    @(await Component.InvokeAsync("HeaderPrimary"))

    <main role="main" class="main-wrapper pb-3">
        @RenderBody()

        <div class="backdrop hide"></div>
    </main>

    @(await Component.InvokeAsync("FooterPrimary"))

    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

